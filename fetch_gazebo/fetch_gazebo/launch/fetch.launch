<launch>

  <arg name="robot" default="fetch"/>
  <arg name="debug" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>

  <!-- Start Gazebo with a blank world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="debug" value="$(arg debug)" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="paused" value="true"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="headless" value="$(arg headless)"/>
  </include>

<node name="table_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
args="-file $(find fetch_gazebo)/models/table/table.sdf -sdf -x 1.15 -y 0 -z 0  -R 0 -P 0 -Y 1.57 -model table"/>

<node name="blue_cube_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
args="-file $(find fetch_gazebo)/models/blue_cube/blue_cube.sdf -sdf -x 0.85 -y -0.25 -z 0.7  -R 0 -P 0 -Y 0 -model blue_cube"/>
<node name="green_cube_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
args="-file $(find fetch_gazebo)/models/green_cube/green_cube.sdf -sdf -x 0.85 -y 0 -z 0.7  -R 0 -P 0 -Y 0 -model green_cube"/>
<node name="cylinder_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
args="-file $(find fetch_gazebo)/models/cylinder/cylinder.sdf -sdf -x 0.85 -y 0.25 -z 0.7  -R 0 -P 0 -Y 0 -model cylinder"/>
<!-- <node name="box_spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" -->
<!-- args="-file $(find fetch_gazebo)/models/box/box.sdf -sdf -x 0.15 -y -0.4 -z 0.2 -R 1.57 -P -1.57 -Y 0 -model box"/> -->

  <!-- Oh, you wanted a robot? -->
  <include file="$(find fetch_gazebo)/launch/include/$(arg robot).launch.xml" />
<!-- <include file="$(find fetch_moveit_config)/launch/move_group.launch"/> -->
  <node name="fetch_gui" pkg="fetch_gazebo" type="fetch_gui.py" output="screen"/>
  <node name="fetch_obj_detection" pkg="fetch_gazebo" type="fetch_sensor.py" output="screen" />
  <!-- <include file="$(find fetch_gazebo)/launch/iksolver_node.launch" /> -->

</launch>
